@model IEnumerable<VendingNEA_0.Models.CondicionComision>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Porcentaje)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodCondicionNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Porcentaje)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodCondicionNavigation.CodCondicion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodCondicion">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodCondicion">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodCondicion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
